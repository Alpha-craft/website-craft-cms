<div class="bg-green-400 flex flex-col rounded p-5 my-3" >
  <div class="text-center border-b-2 border-black" > {{group}} </div>
  {% block card %}  
  {% endblock %}

  <ul>
    {% set items = items|default([]) %}
    {% for item in items if item.label is not empty %}
      <li> {{ item.label|raw }} </li>
    {% endfor %}
  </ul>
  {% set others = others|default("") %}
    {{others}}
</div>